{
    "name": "@wq/map-gl-native",
    "version": "3.0.0-alpha.0",
    "description": "React Native & Expo bindings for @wq/map-gl",
    "type": "module",
    "main": "src/index.js",
    "scripts": {
        "test": "cd ../../ && npm run jest packages/map-gl-native -- --config packages/map-gl-native/jest.config.js --moduleDirectories node_modules packages/map-gl-native/node_modules",
        "build": "npm run prettier",
        "prettier": "cd ../../ && npm run prettier -- --write packages/map-gl-native/",
        "lint": "cd ../../ && npm run eslint packages/map-gl-native/{,src/,src/*/,src/*/*/}*.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/wq/map-gl.git",
        "directory": "packages/map-gl-native"
    },
    "keywords": [
        "wq",
        "mapbox",
        "map",
        "gl",
        "webgl",
        "gis"
    ],
    "author": "S. Andrew Sheppard",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/wq/map-gl/issues"
    },
    "homepage": "https://map-gl.wq.io/@wq/map-gl",
    "dependencies": {
        "@wq/react": "^3.0.0-alpha.0"
    },
    "peerDependencies": {
        "@maplibre/maplibre-react-native": "*",
        "expo-location": "*"
    },
    "devDependencies": {
        "@maplibre/maplibre-react-native": "^10.0.1",
        "expo-location": "^18.0.5",
        "jest-expo": "^52.0.3"
    }
}
