{
    "name": "@wq/map-gl-web",
    "version": "3.0.0-alpha.0",
    "description": "Web bindings for for @wq/map-gl",
    "type": "module",
    "main": "index.js",
    "scripts": {
        "test": "cd ../../ && npm run jest packages/map-gl-web",
        "build": "npm run babel && npm run prettier",
        "babel": "cd ../../ && npm run babel -- packages/map-gl-web/src --out-dir packages/map-gl-web/",
        "prettier": "cd ../../ && npm run prettier -- --write packages/map-gl-web/",
        "lint": "cd ../../ && npm run eslint packages/map-gl-web/{,src/,src/*/,src/*/*/}*.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/wq/map-gl.git",
        "directory": "packages/map-gl-web"
    },
    "keywords": [
        "wq",
        "mapbox",
        "map",
        "gl",
        "webgl",
        "gis"
    ],
    "author": "S. Andrew Sheppard",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/wq/map-gl/issues"
    },
    "homepage": "https://map-gl.wq.io/@wq/map-gl",
    "dependencies": {
        "@turf/bbox": "^7.3.1",
        "@turf/centroid": "^7.3.1",
        "@turf/union": "^7.3.1",
        "@wq/react": "^3.0.0-alpha.0",
        "react-map-gl": "^8.1.0"
    }
}
